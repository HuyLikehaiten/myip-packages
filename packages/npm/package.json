{
  "name": "myip-foo",
  "version": "1.0.0",
  "description": "Official client library for myip.foo - free IP lookup API with geolocation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/react.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ip",
    "ip-address",
    "geolocation",
    "ipv4",
    "ipv6",
    "dual-stack",
    "vpn-detection",
    "myip",
    "react",
    "hooks"
  ],
  "author": "VirtualOx B.V. <hello@virtualox.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virtualox/myip-packages.git",
    "directory": "packages/npm"
  },
  "bugs": {
    "url": "https://github.com/virtualox/myip-packages/issues"
  },
  "homepage": "https://myip.foo",
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
